{
    "name": "valtu4a/typograph",
    "description": "PHP версия типографа Муравьева",
    "keywords": ["типограф", "Муравьев", "typograph", "russian", "русский"],
    "license": "Public Domain",
    "version": "1.1.0",
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Emuravjev\\Mdash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}